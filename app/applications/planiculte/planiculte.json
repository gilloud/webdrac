                                                    {
                                                        "id": 1,
                                                        "name": "Disponible"
                                                    },
                                                    {
                                                        "id": 2,
                                                        "name": "Non disponible"
                                                    },
                                                    {
                                                        "id": 3,
                                                        "name": "Si n\u00e9cessaire"
                                                    }
                                                ]
                                            }
                                        },
                                        "object1": "culte",
                                        "object2": "users"
                                    },
                                    "source": "intersection"
                                },
                                "title": "S\u00e9lection"
                            }
