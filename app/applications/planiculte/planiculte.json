                    "type": "textarea"
                },
                "date": {
                    "description": "Date du culte",
                    "display_name": "Date",
                    "parameters": {
                        "pikaday": {
                            "firstDay": 1,
                            "formatDisplay": "DD-MM-YYYY HH:mm",
                            "formatModel": "YYYY-MM-DD HH:mm:ssZ",
                            "showTime": true,
                            "use24hour": false
