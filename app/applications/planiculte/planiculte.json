                        "length": 100
                    },
                    "type": "string"
                },
                "user": {
                    "description": "Personne ayant la comp\u00e9tence",
                    "display_name": "Personne",
                    "parameters": {},
                    "type": "user"
                }
            },
            "description": "Instrument, Chant ou sonorisation",
            "display_name": "Capacite",
            "list": {
                "all": {
                    "groups": [
                        "all"
                    ],
                    "parameters": {
                        "columns": [
                            "level",
                            "name",
                            "id",
                            "_application",
                            "_entity"
                        ],
                        "columns_details": [
                            {
                                "formatter": "jqgrid_level",
                                "index": "level",
                                "name": "level",
                                "sorttype": "int"
                            },
                            {
                                "index": "name",
                                "name": "name"
                            },
                            {
                                "formatter": "jqgrid_getLink",
                                "index": "idf",
                                "name": "Aller a la fiche"
                            },
                            {
                                "hidden": true,
                                "index": "_application",
                                "name": "_application"
                            },
                            {
                                "hidden": true,
                                "index": "_entity",
                                "name": "_entity"
                            }
                        ],
                        "filters": {}
                    },
                    "title": "Toutes les capacit\u00e9s",
                    "type": "report"
                }
            },
            "name": "capacite",
            "transition": {
                "add": {
                    "from": null,
                    "groups": [
                        "manager",
                        "administrator",
                        "all"
                    ],
                    "parameters": {
                        "attributes": [
                            "name",
                            "level",
                            "user"
                        ]
                    },
                    "title": "Ajouter une capacit\u00e9",
                    "to": "created"
                }
            }
        },
        "culte": {
