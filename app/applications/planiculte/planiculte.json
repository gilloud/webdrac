                "futur": {
                    "groups": [
                        "all"
                    ],
                    "parameters": {
                        "columns": [
                            {
                                "parameters": {
                                    "source": "db"
                                },
                                "title": "date"
                            },
                            {
                                "parameters": {
                                    "source": "db"
                                },
                                "title": "commentaire"
                            },
                            {
                                "parameters": {
