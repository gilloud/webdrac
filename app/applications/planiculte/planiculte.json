                        "columns_details": [
                            {
                                "formatter": "jqgrid_getLink",
                                "index": "date",
                                "name": "date",
                                "sorttype": "int"
                            },
                            {
                                "index": "commentaire",
                                "name": "commentaire"
                            },
                            {
                                "formatter": "jqgrid_Selector",
